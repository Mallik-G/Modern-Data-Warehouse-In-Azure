{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "mdwa-datafactory"
		},
		"mdwa_azure_sql_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mdwa_azure_sql_database'"
		},
		"mdwa_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'mdwa_datalake'"
		},
		"mdwa_control_database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mdwa_control_database'"
		},
		"mdwa_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mdwastorage.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"mdwa_azure_sql_database_connectionString": {
						"value": "[parameters('mdwa_azure_sql_database_connectionString')]"
					},
					"mdwa_datalake_accountKey": {
						"value": "[parameters('mdwa_datalake_accountKey')]"
					},
					"mdwa_control_database_connectionString": {
						"value": "[parameters('mdwa_control_database_connectionString')]"
					},
					"mdwa_datalake_properties_typeProperties_url": {
						"value": "[parameters('mdwa_datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}