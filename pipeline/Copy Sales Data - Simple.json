{
	"name": "Copy Sales Data - Simple",
	"properties": {
		"activities": [
			{
				"name": "Copy Sales Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"sqlWriterStoredProcedureName": "[Clean].[CleanSales]"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DirtyCSVFile",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "mdwa_sql_database_simple",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}